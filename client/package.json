    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.10",
    "parse5": "^7.2.0",
    "prettier": "^3.3.3",
    "protractor": "^7.0.0",
    "rimraf": "^6.0.1",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-scss": "^14.1.0",
