{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "predeploy": "npm run build:prod",
    "deploy": "ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "is-ci || husky",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.8",
    "@angular/cdk": "^18.2.8",
    "@angular/common": "^18.2.8",
    "@angular/compiler": "^18.2.8",
    "@angular/core": "^18.2.8",
    "@angular/forms": "^18.2.8",
    "@angular/material": "^18.2.8",
    "@angular/material-moment-adapter": "^18.2.8",
    "@angular/platform-browser": "^18.2.8",
    "@angular/platform-browser-dynamic": "^18.2.8",
    "@angular/router": "^18.2.8",
    "@microsoft/signalr": "^8.0.7",
    "@ng-matero/extensions": "^18.2.3",
    "@ng-matero/extensions-moment-adapter": "^18.0.0",
    "@ngx-file-upload/core": "^8.0.0",
    "@ngx-file-upload/ui": "^8.0.0",
    "@ngx-formly/core": "^6.3.8",
    "@ngx-formly/material": "^6.3.8",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@r-hannuschka/ngx-fileupload": "^3.4.3",
    "angular-clock": "^0.7.0",
    "angular-in-memory-web-api": "~0.18.0",
    "angular-material-fileupload": "^3.0.2",
    "apexcharts": "^3.54.0",
    "build": "^0.1.4",
    "guid-typescript": "^1.0.9",
    "leaflet": "^1.9.4",
    "leaflet-contextmenu": "^1.4.0",
    "leaflet-draw": "^1.0.4",
    "leaflet-geometryutil": "^0.10.3",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet-semicircle": "^2.0.4",
    "leaflet-switch-basemap": "^1.0.6",
    "leaflet-toolbar": "^0.4.0-alpha.2",
    "leaflet.marker.highlight": "^0.0.3",
    "leaflet.sidepanel": "^1.2.1",
    "moment": "^2.30.1",
    "ng-new-files-uploader": "^1.0.0",
    "ngx-file-drop": "^16.0.0",
    "ngx-permissions": "^17.1.0",
    "ngx-progressbar": "^12.0.2",
    "ngx-toastr": "^19.0.0",
    "photoviewer": "^3.10.3",
    "proj4leaflet": "^1.0.2",
    "rxjs": "~7.8.1",
    "screenfull": "^6.0.2",
    "three": "^0.171.0",
    "tslib": "^2.7.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.8",
    "@angular-eslint/builder": "^18.3.1",
